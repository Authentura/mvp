{
  "name": "authenguard",
  "publisher": "Authentura",
  "displayName": "Authenguard",
  "description": "Authenguard delivers real-time vulnerability detection in your code, along with easy to understand explanations and fixes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "authenguard.scan",
        "title": "Authenguard: Scan for potential vulnerabilities"
      },
      {
        "command": "authenguard.onoff",
        "title": "Toggle the authenguard exntesion"
      }
    ],
    "configuration": {
      "title": "Authenguard",
      "properties": {
        "authenguard.username": {
          "type": "string",
          "title": "Username",
          "description": "Your username for the extension. If you don't have one, get a registration link from our team to sign up.",
          "default": "",
          "scope": "resource"
        },
        "authenguard.token": {
          "type": "string",
          "title": "Token",
          "description": "Access token for using the extension. This should be shown to you when you register. If you need a new one, visit https://mvp.authentura.co.uk/login",
          "default": "",
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "request": "^2.88.2",
    "vsce": "^2.15.0"
  }
}